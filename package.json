{
  "name": "vitepress-plugin-bprogress",
  "version": "1.0.0",
  "description": "A VitePress plugin for beautiful progress bars using bprogress",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "./style": "./lib/index.css"
  },
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
